{
  "configurations": [
    {
      "name": "linux-gcc-x86",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include/**",
        "~/project/**",
        "/usr/lib/**"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x86",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Wpedantic"
      ]
    }
  ],
  "version": 4
}